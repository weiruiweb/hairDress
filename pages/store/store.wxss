/**store_list.wxss**/
page{
	background:#f5f5f5;
}
.store_list{
	padding:40rpx 24rpx 20rpx 24rpx;
	background:#ffff;
	border-bottom:solid 1px #ececec;

}
.store_name{
	font-size:28rpx;
	color:#333;
}
.store_address{
	height:65rpx;
	line-height:65rpx;
}
.store_address,.store_phone{
	font-size:24rpx;
	color:#666;
}
.store_right image{
	width:50rpx;
	height:48rpx;
	margin-top:20rpx;
	margin-right:10rpx;
}
.distance{
	margin-top:20rpx;
}