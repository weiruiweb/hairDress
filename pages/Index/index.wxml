<!--index.wxml-->
<view class="banner">
  <image class="bannerImg" src="/images/banner.jpg"></image>
</view>
<view class="module flexRowBetween" >
  <view class="module_item" bindtap="HairDresser">
    <view class="module_icon module1">
      <image src="/images/index_icon1.jpg" class="firstImg"></image>

    </view>
    <text>预约技师</text>
  </view>
  <view class="module_item" bindtap="shoppingEmpty">
    <view class="module_icon module2">
      <image src="/images/index_icon2.jpg" class="firstImg"></image>

    </view>
    <text>会员权益</text>
  </view>
  <view class="module_item" bindtap="group">
    <view class="module_icon module3">
      <image src="/images/index_icon3.jpg" class="firstImg"></image>

    </view>
    <text>团购活动</text>
  </view>
  <view class="module_item" bindtap="about">
    <view class="module_icon module4">
      <image src="/images/index_icon4.jpg" class="firstImg"></image>

    </view>
    <text>关于我们</text>
  </view>
  <view class="module_item">
   <view class="module_icon module5">
      <image src="/images/index_icon5.jpg" class="firstImg"></image>

    </view>
    <text>联系我们</text>
  </view>
</view>

<view class="store">
  <view class="store_text1" >
    门店
  </view>
  <view class="near" bindtap="store">
    <image src="/images/stores_icon.jpg"></image>
    <view class="text1">最近</view>
  </view>
  <text class="store_active">高新一号店，会员优惠日充200送200~</text>
  <view class="map" bindtap="map"> 
    <image src="/images/stores1.jpg"></image>
  </view>
</view>
<view class="discont">
  <view class="discont_tlt flexRowBetween">
      <view class="tlt_left"></view>
      <view class="tlts">优惠劵</view>
      <view class="tlt_left"></view>
  </view>
  <view class="discount_card flexRowBetween">

      <image src="/images/discont1.jpg"></image>
      <image src="/images/discont2.jpg" ></image>

  </view>
</view>
<view class="vip_card">
  <image src="/images/card1.jpg"></image>
</view>
