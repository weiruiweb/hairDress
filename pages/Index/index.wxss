	background:#fff;
	line-height:88rpx;
	padding:0rpx 55rpx;
}

.near image{
	width:85rpx;
	height:55rpx;
	position:absolute;
	left:0rpx;
	top:0rpx;
}
.store_text1{
	font-size:28rpx;
	color:#666;
	float:left;
}
.near{
	float:left;
	margin-top:18rpx;
	position:relative;
	margin-left:6rpx;}
.near .text1{
	width:60rpx;
	font-size:24rpx;
	color:#fff;
	position:absolute;
	left:20rpx;
	line-height:55rpx;
	z-index:99;
}
.store_active{
	font-size:22rpx;
	color:#999;
	float:left;
	margin-left:100rpx;
	padding-right:46rpx;
	height:30rpx;
	line-height:30rpx;
	border-right:solid 1px #666;
	margin-top:27rpx;
}
.map image{
	width:35rpx;
	height:32rpx;
	margin-left:20rpx;
}
