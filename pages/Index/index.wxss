/**index.wxss**/
page{
	background:#f5f5f5;
}
.banner{position:relative;}
.bannerImg{
	width:100%;
	height:288rpx;}


.custom{
	width:92rpx;
	height:21rpx;
	border-radius:20rpx;
	background:#3497fd;
	line-height:21rpx;
	text-align:center;
	font-size:16rpx;
	color:#fff;
	position:absolute;
	right:276rpx;
	bottom:68rpx;
}

.module{
	padding:0rpx 55rpx;
	margin:40rpx 0rpx 50rpx;}
.module_icon{
	width:84rpx;
	height:84rpx;
	border-radius:50%;
	position:relative;
	overflow:hidden;
	transition:0.5s ease-in-out;
}
.module_item text{color:#151515;font-size:20rpx;text-align:center;}
.module_item .firstImg,.module_item .secondImg{
	transition:0.5s ease-in-out;
}

.module_icon image{
	width:40rpx;
	height:36rpx;
	position:absolute;
	transition: all .5s ease-in-out;
	}
.firstImg{
	left:20rpx;
	top:25rpx;
	}

.module1{background:#4fa8ff; transition:0.5s ease-in-out;}

.module2{background:#ff7979; transition:0.5s ease-in-out;}

.module3{background:#fdd66d; transition:0.5s ease-in-out;}


.module4{background:#d1a3e1; transition:0.5s ease-in-out;}

.module5{background:#4fe0ff; transition:0.5s ease-in-out;}

/********************服务类别*************************/
.store{
